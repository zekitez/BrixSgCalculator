<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.zekitez.brixsgcalculator"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/brix"
        android:label="@string/app_name"
        android:name="com.zekitez.brixsgcalculator.GlobalFunctions"
        android:roundIcon="@mipmap/brix"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.zekitez.brixsgcalculator.PrefsActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>